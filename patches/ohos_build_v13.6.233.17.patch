+  } else {
+    assert(false, "Architecture not supported")
+  }
+
+  if (use_musl) {
+    cflags += [ "--target=$abi_target" ]
+    if (use_custom_libcxx) {
+      include_dirs = [ "$ohos_sysroot/usr/include/$abi_target" ]
+    } else {
+      include_dirs = [
+        "$ohos_toolchain_root/include/libcxx-ohos/include/c++/v1",
+        "$ohos_sysroot/usr/include",
+        # musl libc headers (e.g. bits/alltypes.h) live under the target include dir.
+        "$ohos_sysroot/usr/include/$abi_target",
